{
    "background": {
        "persistent": false,
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "js": [
                "inject.js"
            ],
            "matches": [
                "\u003Call_urls>"
            ],
            "run_at": "document_start"
        },
        {
            "all_frames": true,
            "js": [
                "inject.js"
            ],
            "match_about_blank": true,
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "run_at": "document_start"
        }
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "description": "Adds Orbita debugging tools to the Chrome Developer Tools.",
    "devtools_page": "main.html",
    "manifest_version": 2,
    "minimum_chrome_version": "43",
    "name": "Orbita Developer Tools",
    "permissions": [
        "file:///*",
        "http://*/*",
        "https://*/*"
    ],
    "version": "0.0.1",
    "web_accessible_resources": [
        "main.html"
    ]
}